{"_id":{"$oid":"56229978fd0afd2a8e70edbc"},"checksum":"","createDate":{"$date":"2015-10-17T18:55:05.140Z"},"creatorId":"","data":{"form":"    \u003cscript\u003e\n        //Add Extensions to the IndexController Behavior Here with the Controller.reopen method.//\n        window.App.IndexController.reopen({\n            messageBlock: \"\",\n            isHistoryOpen: false,\n            dataResults: function(){\n                console.log(this.get('facts.RESULTS'));\n                var arrayResults = Ember.A();\n                this.get('facts.RESULTS').forEach( function(item){\n                    arrayResults.push(JSON.stringify(item));\n                });\n                return arrayResults;\n            }.property(),\n            messageHistory: function(mystage, questionIdx) {\n                var history = this.get(\"model.HISTORY\");\n                var myMsgBlock = this.get(\"messageBlock\") + '\u003cul\u003e';\n                history.forEach( function(item) {\n                    myMsgBlock = myMsgBlock + '\u003cli\u003eTask#\u0026nbsp;' + item.ID + '\u0026nbsp;\u0026nbsp;' + item.LABEL + ':\u0026nbsp;' + item.DESCRIPTION + '\u0026nbsp;Status\u0026nbsp;' + item.STATUS + '\u003c/li\u003e';\n                });\n                myMsgBlock = myMsgBlock + '\u003c/ul\u003e';\n\n                this.set(\"messageBlock\", myMsgBlock);\n                var d = $('.history-content');\n                d.scrollTop(d.prop(\"scrollHeight\"));\n                return myMsgBlock;\n            }.property(\"model.HISTORY\"),\n            actions: {\n                cancelAction: function() {\n                    $('[name=optionid]').val('reject');\n                    this.set(\"isSubmitting\", true );\n                    // here you could perform your actions like persisting to the server or so\n                    $('#emberForm').submit();\n                },\n                toggleHistory: function () {\n                    console.log(\"Current State: \" + this.get(\"isHistoryOpen\"));\n                    var historyState = this.get(\"isHistoryOpen\");\n                    if (historyState) {\n                        $('#openIcon').removeClass('glyphicon-expand').addClass('glyphicon-collapse-down');\n                    } else {\n                        $('#openIcon').removeClass('glyphicon-collapse-down').addClass('glyphicon-expand');\n                    }\n                    this.set(\"isHistoryOpen\", !historyState);\n                }\n            }\n        });\n    \u003c/script\u003e\n    \u003cscript type=\"text/x-handlebars\" data-template-name=\"index\"\u003e\n        \u003cdiv class=\"container\" class=\"form-horizontal\"\u003e\n            \u003cdiv id=\"content panel-group\" id=\"accordion\"\u003e\n                \u003cdiv style=\"padding: 10px;\"\u003e\n                    \u003cdiv class=\"panel panel-default\"\u003e\n                        \u003cdiv class=\"panel-heading\"\u003e\n                            \u003ch3 class=\"panel-title\"\u003eData Returned\u003c/h3\u003e\n                        \u003c/div\u003e\n                        \u003cdiv id='bod' class=\"panel-body\"\u003e\n                            \u003cdiv class=\"row\"\u003e\n                                \u003col\u003e\n                                {{#each row in controller.dataResults}}\n                                \u003cli\u003e{{row}}\u003c/li\u003e\n                                {{/each}}\n                                \u003c/ol\u003e\n                            \u003c/div\u003e                            \n                            \u003cspan class=\"pull-right clickable\" {{action 'toggleHistory'}}\u003eRequest History\u0026nbsp;\u003ci id=\"openIcon\" class=\"glyphicon glyphicon-expand\"\u003e\u003c/i\u003e\u003c/span\u003e\n                        \u003c/div\u003e\n                    \u003c/div\u003e\n                    \u003cdiv align=\"center\"\u003e\n                        \u003cbutton id=\"approve\" class=\"btn btn-success\" {{action 'submitAction' target=\"controller\"}}\u003eSubmit\u003c/button\u003e\n                        \u003cbutton id=\"reject\" class=\"btn btn-danger\" {{action 'cancelAction' target=\"controller\"}}\u003eCancel\u003c/button\u003e\n                    \u003c/div\u003e\n                    {{#if isHistoryOpen}}\n                    \u003cdiv class=\"panel panel-info panel-collapsed\"\u003e\n                        \u003cdiv class=\"panel-heading\"\u003e\n                            \u003ch3 class=\"panel-title\"\u003eRequest History\u003c/h3\u003e\n                        \u003c/div\u003e\n                        \u003cdiv class=\"panel-body\"\u003e\u003cspan class=\"history-content\"\u003e{{{messageHistory}}}\u003c/span\u003e\n                        \u003c/div\u003e\n                    \u003c/div\u003e\n                    {{/if}}\n                \u003c/div\u003e\n            \u003c/div\u003e \u003c!-- content --\u003e\n        \u003c/div\u003e \u003c!-- container --\u003e\n        {{input type=\"hidden\" name=\"optionid\"}}\n    \u003c/script\u003e"},"desc":"Recently Converted Form Record","id":"56229978fd0afd2a8e70edbc","name":"show data \u0026 submit","origin":{"checksum":"unknown","createDate":0,"creatorId":"","desc":"","id":"","name":""},"owner":{},"recType":"form","tstamp":0,"updateDate":{"$date":"2015-10-17T18:55:05.140Z"},"updatorId":""}
