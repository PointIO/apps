"><extensionElements><activiti:field name=\"PluginKey\"><activiti:string>poi_FactRead_fact</activiti:string></activiti:field><activiti:field name=\"parameterMap\"><activiti:expression>${READMap}</activiti:expression></activiti:field></extensionElements></serviceTask><sequenceFlow targetRef=\"userTask11\" sourceRef=\"serviceTask6\" id=\"flow2\"/><userTask activiti:assignee=\"${wf_currentAssignee}\" activiti:formKey=\"poi_UserView_form\" name=\"User View\" id=\"userTask11\"/><boundaryEvent attachedToRef=\"userTask11\" cancelActivity=\"false\" name=\"Message\" id=\"boundarymessage3\"><messageEventDefinition messageRef=\"injectListJSON\"/></boundaryEvent><sequenceFlow targetRef=\"end\" sourceRef=\"userTask11\" id=\"flow4\"/><endEvent name=\"end\" id=\"end\"/></process></definitions>"}}],"forms":[{"_id":{"$oid":"564df4bb3c0000a910d3b219"},"id":"564df4bb3c0000a910d3b219","owner":{},"recType":"app","name":"Create/Edit Contact List","desc":"View and modify contact list for apps","creatorId":"msalomone@point.io","updaterId":"msalomone@point.io","tstamp":1448046556227,"createDate":{"$date":1447949499318},"updateDate":{"$date":1448046556227},"origin":{"id":"","name":"Blast Email List","desc":"View and modify email list for blast","creatorId":"msalomone@point.io","createDate":1447949499318,"checksum":"FCB7B75C20D28E3E988EB6DFB1AD1E445636AC7D8A64F084DFB068AB6D67A8D2L12311"},"data":{"form":"\n<!-- @@@@@@ Start Form  @@@@@@ -->\n\n<script type=\"text/javascript\">\n\n    var users = jsonTask.FACTS.RESULTS;\n\n    App.Router.map(function () {\n        this.resource('index', {path: '/'});\n        this.resource('new', {path: '/new'});\n        this.resource('update', {path: '/:key'});\n    });\n    window.App.IndexController.reopen({\n        isSubmitable: false,\n        facts: jsonTask.FACTS,\n\n        actions:{\n                postAction: function () {\n                    var matchQuery = {\"recordType\": facts.apidata_data.recordType, \"listName\": \"contactList\"}\n                    var updateQuery = {\"recordType\": facts.apidata_data.recordType, \"contactList\": facts.apidata_data.contactList, \"listName\": \"contactList\"}\n                    var baseURL = window.document.location.protocol + \"//\" + window.document.location.host;\n                    var sessionKey  = facts.wf_origSessionKey;\n                    var data = {\"match\":matchQuery, \"update\":updateQuery}\n                    var servercall = baseURL + \"/objectstore/upsert?Authorization=\" + sessionKey;\n                    $.ajax({\n                        type: \"POST\",\n                        data: JSON.stringify(data),\n                        contentType: \"application/json; charset=utf-8\",\n                        url: servercall,\n                        success: function (data) {\n                          if(data.RESPONSE.ERR == 0 ){\n                                   console.log(\"Insert data successfully: \" + data.RESPONSE.objId);\n                                   $('[name=wf_optionsOutcome]').val('submit');\n                                    $('#emberForm').submit();\n                                }\n                              },\n                              error:function(err){\n                                console.log(err);\n                              }\n                });\n            }\n        },\n\n        init: function() {\n            var self = this;\n            var myfacts = this.get('facts');\n            //console.log(myfacts);\n            this.set(\"facts\", myfacts);\n            if(facts.apidata_data.contactList == null){\n                facts.apidata_data.contactList = Ember.A();\n                //console.log(facts);\n            };\n            if(facts.apidata_data.recordType == null){\n                this.set('facts.apidata_data.recordType', \"contactList\");\n                //console.log(facts);\n            }\n        }\n    });\n\n    App.IndexRoute = Ember.Route.extend({\n        controllerName: 'index',\n        setupController: function(controller, model){\n            console.log(this.get(\"controller\").get(\"facts\"));\n        },\n        renderTemplate: function(controller) {\n            this.render('index', {controller: controller});\n        },\n        model: function(params) {\n            var templateObjects = Ember.A();\n            return templateObjects;\n\n        },\n\n        actions: {\n\n        }\n    });\n\n    App.NewRoute = Ember.Route.extend({\n        controllerName: 'index',\n        setupController: function(controller, model){\n            //console.log(this.get(\"controller\").get(\"facts\"));\n            controller.set('model', model);\n        },\n        renderTemplate: function(controller) {\n            this.render('new', {controller: controller});\n        },\n        model: function(params) {\n            var templateObject = {\n                firstName: \"\",\n                lastName: \"\",\n                email: \"\",\n            };\n            return templateObject;\n        },\n\n        actions: {\n            create: function(){\n                var model = this.get('controller').get('model');\n                var myfacts = this.get('controller').get('facts');\n                myfacts.apidata_data.contactList.pushObject(model);\n                this.get('controller').set(\"facts\", myfacts);\n                this.transitionTo('index');\n            }\n        }\n    });\n\n    App.UpdateRoute = Ember.Route.extend({\n        controllerName: 'index',\n        setupController: function(controller, model){\n            var myfacts = this.get('controller').get('facts');\n            //console.log(myfacts);\n            var templateObject = {};\n            var array = myfacts.apidata_data.contactList;\n            for(var i = 0; i < array.length; i++) {\n                if(array[i].email == model.email) {\n                    console.log(array[i]);\n                    templateObject = {\n                        firstName: array[i].firstName,\n                        lastName: array[i].lastName,\n                        email: array[i].email\n                    };\n                    break;\n                }\n            }\n            //console.log(templateObject);\n            controller.set('model', templateObject);\n        },\n        renderTemplate: function(controller) {\n            this.render('update', {controller: controller});\n        },\n        model: function(params) {\n            var templateObject = {};\n            templateObject = {\n                email: params.key\n            };\n            console.log(templateObject);\n            return templateObject;\n        },\n\n        actions: {\n            update: function(mod){\n                var myfacts = this.get('controller').get('facts');\n                var array = myfacts.apidata_data.contactList;\n                for(var i = 0; i < array.length; i++) {\n                    if(array[i].email == mod.email) {\n                        array[i] = {\n                            firstName: mod.firstName,\n                            lastName: mod.lastName,\n                            email: mod.email\n                        };\n                        break;\n\n                    }\n                }\n                Ember.set(myfacts.apidata_data.contactList, array);\n                this.get('controller').set(\"facts\", myfacts);\n                this.transitionTo('index');\n            }\n        }\n    });\n\n\n\n</script>\n\n\n<script type=\"text/x-handlebars\" data-template-name=\"index\">\n\t<div class=\"container\">\n        <div class=\"row\" style=\"margin-right: 0px;\">\n            <div class=\"col-md-12\" style=\"padding-right: 35px;\">\n                <div class=\"pull-right\" style=\"margin-top: 10px\">\n                    {{#link-to 'new'}}\n                        <input type=\"button\" class=\"btn btn-success\" value=\"Add New User\" style=\"background-color: #1ab394; border-color: #1ab394;\"/>\n                    {{/link-to}}\n                </div>\n                <table class=\"table table-responsive\">\n                    <thead>\n                        <tr>\n                            <th>First Name</th>\n                            <th>Last Name</th>\n                            <th>Email</th>\n                            <th>Action</th>\n                        </tr>\n                    </thead>\n                    <tbody>\n                        {{#each item in facts.apidata_data.contactList}}\n                        <tr>\n                            <td>\n                                {{item.firstName}}\n                            </td>\n                            <td>\n                                {{item.lastName}}\n                            </td>\n                            <td>\n                                {{item.email}}\n                            </td>\n                            <td>\n                                {{#link-to 'update' item.email}}\n                                <input type=\"button\" class=\"btn btn-success\" value=\"Edit\"/>\n                                {{/link-to}}\n                            </td>\n                        </tr>\n                        {{/each}}\n                    </tbody>\n                </table>\n            </div>\n        </div>\n    </div>\n\n    <div class=\"row\">\n        <div class=\"row text-center\">\n            <button type=\"submit\" {{action 'postAction'}} class=\"btn btn-primary\">\n                <span class=\"glyphicon glyphicon-ok\"></span>&nbsp; Submit\n            </button>\n        </div>\n    </div>\n</script>\n\n<script type=\"text/x-handlebars\" data-template-name=\"new\">\n    <div class=\"container\">\n        <div class=\"row\"  style=\"margin-right: 0px\">\n            <div class=\"col-md-12\" style=\"padding-right: 35px;\">\n                <table class=\"table table-responsive\">\n                    <thead>\n                        <tr>\n                            <th>First Name</th>\n                            <th>Last Name</th>\n                            <th>Email</th>\n                        </tr>\n                    </thead>\n                    <tbody>\n                        <tr>\n                            <td>\n                                {{input value=model.firstName class=\"form-control\"}}\n                            </td>\n                            <td>\n                                {{input value=model.lastName class=\"form-control\"}}\n                            </td>\n                            <td>\n                                {{input value=model.email class=\"form-control\"}}\n                            </td>\n                        </tr>\n                    </tbody>\n                </table>\n            </div>\n        </div>\n    </div>\n\n    <div class=\"row\">\n        <div class=\"row text-center\">\n            <button type=\"button\" onclick=\"history.back(-1)\" class=\"btn btn-primary\">\n                <span title=\"goback\" class=\"glyphicon glyphicon-arrow-left\"></span>&nbsp; Back\n            </button>&nbsp;\n            <button type=\"button\" class=\"btn btn-primary\" {{action \"create\" model}}>\n                <span class=\"glyphicon glyphicon-ok\"></span>&nbsp; Add User\n            </button>\n        </div>\n    </div>\n</script>\n\n<script type=\"text/x-handlebars\" data-template-name=\"update\">\n    <div class=\"container\">\n        <div class=\"row\"  style=\"margin-right: 0px\">\n            <div class=\"col-md-12\" style=\"padding-right: 35px;\">\n                <table class=\"table table-responsive\">\n                    <thead>\n                        <tr>\n                            <th>First Name</th>\n                            <th>Last Name</th>\n                            <th>Email</th>\n                        </tr>\n                    </thead>\n                    <tbody>\n                        <tr>\n                            <td>\n                                {{input value=model.firstName class=\"form-control\"}}\n                            </td>\n                            <td>\n                                {{input value=model.lastName class=\"form-control\"}}\n                            </td>\n                            <td>\n                                {{input value=model.email class=\"form-control\"}}\n                            </td>\n                        </tr>\n                    </tbody>\n                </table>\n            </div>\n        </div>\n    </div>\n\n    <div class=\"row\">\n        <div class=\"row text-center\">\n            <button type=\"button\" onclick=\"history.back(-1)\" class=\"btn btn-primary\">\n                <span title=\"goback\" class=\"glyphicon glyphicon-arrow-left\"></span>&nbsp; Back\n            </button>&nbsp;\n            <button type=\"button\" class=\"btn btn-primary\" {{action \"update\" model}}>\n                <span class=\"glyphicon glyphicon-ok\"></span>&nbsp; Update User\n            </button>\n        </div>\n    </div>\n</script>\n\n\n<!-- @@@@@@ End Form @@@@@@ -->\n","facts":{}},"checksum":"58489011C5A586E7B1B12A4C17C41AB563253F73C80BC74F6ACF2BCBC9E8651FL11796"}]}